generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Users {
  id       String  @id @db.Uuid
  nickname String  @unique
  email    String  @unique
  password String
  valid    Boolean

  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())
}
